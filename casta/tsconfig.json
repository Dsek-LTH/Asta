{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "declarationMap": true,
        "inlineSources": false,
        "declaration": true,
        "stripInternal": true,
        "lib": [
          "es6",
          "dom"
        ],
        "strict": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "rootDir": "static",
        "baseUrl": ".",
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "include": ["static"]
}
